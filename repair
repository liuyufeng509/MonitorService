#!/bin/bash
function check(){
    count=`ps -ef |grep $1 |grep -v "grep" |wc -l`
    #echo $count
    if [ 0 == $count ];then
        nohup  python  /runscript/working/$1 &
    fi
}

nohup /home/liuyufeng/build-MonitorService-Desktop_Qt_5_6_1_GCC_64bit-Debug/MonitorService 
count=`ps -ef |grep MonitorService |grep -v "grep" |wc -l`
#echo $count
if [ 0 == $count ];then
   echo "MonitorService start failed"
else
   echo "MonitorSerivce start success!"
fi  
exit 0
